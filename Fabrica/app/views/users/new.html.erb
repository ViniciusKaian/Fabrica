<%= form_for(@user) do |f| %>
	<p>
		<%= f.label :name %><br>
		<%= f.text_field :name %>
	</p>
	<p>
		<%= f.label :email %><br>
		<%= f.text_field :email %>
	<p>
		<%= f.label :senha %><br>
		<%= f.password_field :senha %><br><br>
		<%= f.password_field :senha_confirmation %>
	</p>
	<p>
		<%= f.submit "Cadastrar" %>
	</p>
<% end %>